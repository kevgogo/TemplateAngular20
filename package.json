{
  "name": "template-plantilla-colibri-app-20",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "start:local": "ng serve --configuration local --host 0.0.0.0 --port 4200 --proxy-config proxy.conf.local.json",
    "start:dev": "ng serve --configuration development",
    "start:qa": "ng serve --configuration qa",
    "start:production": "ng serve --configuration production",
    "start:prod": "npm run start:production",
    "build:local": "ng build --configuration local",
    "build:dev": "ng build --configuration development",
    "build:qa": "ng build --configuration qa",
    "build:prod": "ng build --configuration production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --passWithNoTests",
    "test:ng": "ng test",
    "test:ng:watch": "ng test --watch",
    "test:ng:ci": "ng test --coverage --watch=false --silent",
    "prepare": "husky",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "eslint": "eslint . --ext .ts,.html",
    "eslint:fix:all": "eslint . --ext .ts,.html --fix",
    "eslint:watch": "eslint . --ext .ts,.html --cache",
    "lint:all": "npm run eslint && npm run lint",
    "lint:strict": "eslint . --ext .ts,.html --max-warnings=0 && ng lint --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "precommit:lint": "lint-staged",
    "lint-staged:debug": "lint-staged --debug --concurrent false --verbose",
    "verify": "npm run prettier:check && npm run lint && npm run test -- --passWithNoTests",
    "verify:strict": "npm run prettier:check && npm run lint:strict && npm run test -- --passWithNoTests",
    "ci:local": "npm run build:local && npm run eslint && npm run prettier:check && npm run test -- --ci --passWithNoTests"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "*.{html,scss,css,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/common": "20.2.3",
    "@angular/compiler": "20.2.3",
    "@angular/core": "20.2.3",
    "@angular/forms": "20.2.3",
    "@angular/platform-browser": "20.2.3",
    "@angular/router": "20.2.3",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@sweetalert2/ngx-sweetalert2": "^14.0.0",
    "bootstrap": "^5.3.3",
    "dayjs": "^1.11.13",
    "devextreme": "^24.1.5",
    "devextreme-angular": "^24.1.5",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "ngx-bootstrap": "^20.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "^7.8.1",
    "sweetalert2": "^11.22.5",
    "tslib": "^2.8.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz",
    "xng-breadcrumb": "^14.1.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "20.0.0",
    "@angular/build": "20.2.0",
    "@angular/cli": "20.2.0",
    "@angular/compiler-cli": "20.2.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@types/jest": "^29.5.14",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.41.0"
  }
}
